@article{henkel2014,
  title = {Basic {{Regulatory Principles}} of \textit{Escherichia coli}'s {{Electron Transport Chain}} for {{Varying Oxygen Conditions}}},
  author = {Henkel, Sebastian G. and Beek, Alexander Ter and Steinsiek, Sonja and Stagge, Stefan and Bettenbrock, Katja and {de Mattos}, M. Joost Teixeira and Sauter, Thomas and Sawodny, Oliver and Ederer, Michael},
