@article{patrick2015,
  title = {Free {{RNA}} Polymerase in {{{\emph{Escherichia coli}}}}},
  author = {Patrick, Michael and Dennis, Patrick P. and Ehrenberg, Mans and Bremer, Hans},
  year = {2015},
  month = dec,
  volume = {119},
